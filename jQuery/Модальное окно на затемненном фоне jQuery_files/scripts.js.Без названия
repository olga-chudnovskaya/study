function initCopy(target, value){
	var clipboard = new ClipboardJS(target, {text: function(trigger) {return value();}});
	clipboard.on('success', function(e) {
		$(e.trigger).addClass('copied');
		setTimeout(function(){$(e.trigger).removeClass('copied');}, 2000);
	});
	clipboard.on('error', function(e) {snp_message_danger('<p>  </p>');});
}

$(function(){	
	var $els = $('.block_code-copy');
	if ($els.length > 0) {
		$els.each(function(){ 
			var id = $(this).data('id');
			initCopy(this, function(){
				var res = [];
				if ($("#bcode-" + id).find('.hljs-ln-code').length > 0) {
					$("#bcode-" + id + " .hljs-ln-code").each(function(){res.push($(this).text());});
				} else {
					res.push($("#bcode-" + id).text());
				}
				return res.join("\r\n");
			});
		});
	}	
});

$(function(){	
	$('body').on('click', '.snp-loginbox-triger',function() {
		$('.snp-loginbox-popup').toggle();
		return false;
	});

	$("body").click(function(e) {
		if ($(e.target).closest(".snp-loginbox-popup").length == 0) {
			$(".snp-loginbox-popup").hide();
		}
	});

	$('body').on('click', '.snp-open-login',function(e) {
		snp_form_login('');
		return false;
	});
});

if (window.location.hash === "#_=_"){
    history.replaceState ? history.replaceState(null, null, window.location.href.split("#")[0]) : window.location.hash = "";
}

$(function(){	
	$('footer').append('<img src="/pixel.jpg?v=' + window.location.hostname + '" style="position: absolute;">');
	
	/* bookmarks */
	$('body').on('click', '.snp-bookmark-triger:not(.snp-open-login)', function(){
		var target = $(this);
		$.ajax({
			url: '/api/bookmarks',
			method: 'post',
			dataType: 'json',
			data: {section: target.data('section'), item: target.data('item')},
			success: function(data){ 
				if (data.response == 1) {
					target.attr('title', data.title);
					if (data.active == 1) {
						target.addClass('active');
					} else {
						target.removeClass('active');
					}
					target.find('span').html(data.text);
				} else {
					snp_message_danger(data.error);
				}
			}
		});		
		return false;
	});	

	/* like */
	$('body').on('click', '.snp-like-plus, .snp-like-minus', function(){
		var parent =  $(this).closest('.snp-like');
		var section =  parent.data('section');
		var item =  parent.data('item');
		if ($(this).hasClass("snp-like-plus")) {
			var click = 1;
		} else {
			var click = 2;
		}
		$.ajax({
			url: '/api/likes',
			method: 'post',
			dataType: 'json',
			data: {section: section, item: item, click: click},
			success: function(data){ 
				if (data.response == 1) {
					var id = '.like-' + section + '-' + item + ' ';
					$(id + '.snp-like-plus span').html(data.val_plus);
					$(id + '.snp-like-minus span').html(data.val_minus);
					if (data.state_plus == 1) {
						$(id + '.snp-like-plus').addClass('active');
					} else {
						$(id + '.snp-like-plus').removeClass('active');
					}
					if (data.state_minus == 1) {
						$(id + '.snp-like-minus').addClass('active');
					} else {
						$(id + '.snp-like-minus').removeClass('active');
					}
				} else {
					snp_message_danger(data.error);
				}
			}
		});		
		return false;
	});	

	function snp_comment_form(target){
		var form = {};
		form.editor = $(target).closest('form');
		form.plaintext = form.editor.find('.snp-comment-plaintext');
		form.html = form.plaintext.html();
		form.upload = form.editor.find('.snp-comment-upload');
		return form;
	}

	function snp_comment_dom(target){
		var dom = {};
		dom.item       = $(target).closest('.snp-comment-item');
		dom.sectionId  = dom.item.data('section');
		dom.itemId     = dom.item.data('item');
		dom.id         = dom.item.data('id');
		dom.body       = dom.item.find('.snp-comment-body').first();
		dom.children   = dom.item.find('.snp-comment-list').first();
		dom.like       = dom.body.find('.snp-comment-like').first();
		dom.likeValue  = dom.like.find('span');
		dom.likePlus   = dom.like.find('.snp-comment-plus');
		dom.likeMinus  = dom.like.find('.snp-comment-minus');
		dom.answerform = dom.item.find('.snp-comment-answerform').first();
		dom.bookmark   = dom.body.find('.snp-comment-bookmarks');
		return dom;
	}

	function snp_comment_show(id){
		$('.comment_animation code').each(function(){
			hljs.highlightBlock(this);
		});

		setTimeout (function(){
			$('.snp-comment-body').removeClass('comment_animation');
		}, 2000);	
		
		var comment = $('#comment-' + id);
		var wt = $(window).scrollTop(); 
		var wh = $(window).height();    
		var eh = comment.height();  
		var et = comment.offset().top;
		if (et >= wt && et + eh <= wh + wt){
		
		} else {
			window.location.hash = '#comment-' + id;
		}
	}

	function snp_comment_counter(val){
		var curent = parseInt($('.js-comment-count').text());
		if (isNaN(curent)) {curent = 0;}
		curent = curent + val;
		if (curent == 0) {curent = '';}
		$('.js-comment-count').text(curent);
	}

	function snp_comment_escape(text) {
		var map = {'&': '&amp;','<': '&lt;','>': '&gt;','"': '&quot;',"'": '&#039;'};
		return text.replace(/[&<>"']/g, function(m) { return map[m];});
	}		

	function snp_comment_ajax(options) {
		$.ajax({
			url: options.url,
			method: 'post',
			dataType: 'json',
			data: options.data,
			success: function(data){ 
				if (data.response == 0) {
					snp_message_danger(data.error);
					options.error(data);				
				} else {
					options.success(data);
				}
			}
		});
	}

	$('body').on('paste', '.snp-comment-plaintext',function(e) {
		e.preventDefault();
		text = (e.originalEvent || e).clipboardData.getData('text/plain');
		document.execCommand('insertHtml', false, snp_comment_escape(text));
	});

	$('body').on('click', '.snp-comment-btn-b', function(){
		document.execCommand("bold");
		return false;
	});

	$('body').on('click', '.snp-comment-btn-i', function(){
		document.execCommand("italic");
		return false;
	});	

	$('body').on('click', '.snp-comment-btn-code',function() {
		document.execCommand("insertHTML", false, "<code>" + snp_comment_escape(document.getSelection().toString()) + "</code>");
		return false;
	});

	$('body').on('change', '.snp-comment-btn-img input', function(){
		var form = snp_comment_form(this);
		var length = form.upload.find('.snp-comment-upload-item').length;
		var item = $('<div class="snp-comment-upload-item comment-loading"></div>');
		form.upload.append(item);
		form.editor.ajaxSubmit({
			type: 'POST',
			dataType: 'json',
			url: '/api/comment_upload',
			data: {length: length},
			success: function(data) {
				if (data.response == 0) {
					item.remove();
					snp_message_danger(data.error);
				} else {
					item.removeClass('comment-loading');
					item.html(data.thumb);
					$('.snp-comment-upload').sortable();
				}
			}
		});		
		return false;
	});		
	$('body').on('click', '.snp-comment-upload-remove',function() {
		$(this).closest('.snp-comment-upload-item').remove();
		return false;
	});

	$('body').on('click', '.snp-comment-btn-link', function(){
		var url = prompt('  ', '');
		var res = url.match(/(http(s)?:\/\/.)?(www\.)?[-a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6}\b([-a-zA-Z0-9@:%_\+.~#?&//=]*)/g);
		if (res == null) {
			snp_message_danger('<p>      URL-.</p>');		
		} else {
			document.execCommand('CreateLink', false, url);
		}
		document.execCommand('CreateLink', false, url);
		return false;
	});	

	$('body').on('click', '.snp-comment-btn-blockquote', function(){
		document.execCommand('formatBlock', false, 'blockquote');
		return false;
	});	

	$('body').on('click', '.snp-comment-btn-clear', function(){
		var form = snp_comment_form(this);	
		form.plaintext.html(form.plaintext.text());
		return false;
	});

	$('body').on('keydown', '.snp-comment-plaintext', function(e){
		if (e.keyCode === 9) { 
			e.preventDefault();
			var editor = this;
			var doc = editor.ownerDocument.defaultView;
			var sel = doc.getSelection();
			var range = sel.getRangeAt(0);
			var tabNode = document.createTextNode("\t");
			range.insertNode(tabNode);
			range.setStartAfter(tabNode);
			range.setEndAfter(tabNode); 
			sel.removeAllRanges();
			sel.addRange(range);
		}		
	});

	$('body').on('mousedown', '.snp-comment-editor-toolbar', function() {
		return false;
	});

	$('body').on('click', '.snp-comment-answer',function() {
		if ($(this).attr('href') == '#') {
			var dom = snp_comment_dom(this);
			if (dom.answerform.html().trim() == '') {
				snp_comment_ajax({
					url: '/api/comment_form',
					data: {section: dom.sectionId, item: dom.itemId, id: dom.id},
					success: function(data){ 
						dom.answerform.html(data.html);
					}
				});
			}
			return false;		
		}
	});

	$('body').on('click', '.snp-comment-btn-send',function() {
		var form = snp_comment_form(this);	
		if (form.html.trim() != "") {
			snp_comment_ajax({
				url: '/api/comment_add',
				data: 'text=' + Base64.encode(form.html) + '&' + form.editor.serialize(),
				success: function(data){ 
					if(!form.editor.prev().is('.snp-comment-list')) {form.editor.before('<div class="snp-comment-list"></div>');}
					form.editor.prev('.snp-comment-list').append(data.comment);
					form.plaintext.empty();
					form.upload.empty();			
					snp_comment_show(data.id, true);
					snp_comment_counter(1);
				}
			});
		}
		return false;
	});

	$('body').on('click', '.snp-comment-btn-answer',function() {
		var form = snp_comment_form(this);	
		var dom = snp_comment_dom(this);
		if (form.html.trim() != "") {
			snp_comment_ajax({
				url: '/api/comment_add',
				data: 'text=' + Base64.encode(form.html) + '&' + form.editor.serialize(),
				success: function(data){ 
					if(!dom.body.next().is('.snp-comment-list')) {dom.body.after('<div class="snp-comment-list"></div>');}
					dom.body.next('.snp-comment-list').append(data.comment);
					form.editor.remove();
					snp_comment_show(data.id, true);
					snp_comment_counter(1);
				}
			});
		}
		return false;
	});

	$('body').on('click', '.snp-comment-edit',function(){
		var dom = snp_comment_dom(this);
		snp_comment_ajax({
			url: '/api/comment_edit',
			data: {section: dom.sectionId, item: dom.itemId, id: dom.id},
			success: function(data){ 
				dom.answerform.html(data.html);
				$('.snp-comment-upload').sortable();
			}
		});
		return false;
	});

	$('body').on('click', '.snp-comment-btn-save',function(){
		var dom = snp_comment_dom(this);
		var form = snp_comment_form(this);	
		if (form.html.trim() != "") {
			snp_comment_ajax({
				url: '/api/comment_save',
				data: 'text=' + Base64.encode(form.html) + '&' + form.editor.serialize(),
				success: function(data){ 
					dom.body.replaceWith(data.html);
					window.location.hash = '#comment-' + dom.id;
					snp_comment_show(data.id);
				}
			});
		}
		return false;
	});

	$('body').on('click', '.snp-comment-btn-cancel',function() {
		var form = snp_comment_form(this);
		form.editor.remove();
		return false;
	});

	$('body').on('click', '.snp-comment-remove',function() {
		$(this).parents(".snp-comment-more-popup").hide();
		if (confirm('    ?')) {
			var dom = snp_comment_dom(this);
			snp_comment_ajax({
				url: '/api/comment_remove',
				data: {id: dom.id},
				success: function(data){ 
					dom.body.html(data.html);
					snp_comment_counter(-1);
				}
			});
		}
		return false;
	});

	$('body').on('click', '.snp-comment-restore',function() {
		var dom = snp_comment_dom(this);
		snp_comment_ajax({
			url: '/api/comment_restore',
			data: {id: dom.id},
			success: function(data){ 
				dom.body.replaceWith(data.html);
				snp_comment_show(data.id, true);
				snp_comment_counter(1);
			}
		});
		return false;
	});

	$('body').on('click', '.snp-comment-plus',function(){
		var dom = snp_comment_dom(this);
		var bakup = dom.like.html();
		var rating = Number(dom.likeValue.text());
		if ($(this).hasClass('active')) {
			state = 2;
			rating -= 1;
		} else {
			state = 1;
			rating += 1;
			if (dom.likeMinus.hasClass('active')) {
				dom.likeMinus.removeClass('active');
				rating += 1;
			}
		}
		$(this).toggleClass('active');
		dom.likeValue.attr('class', '');
		if (rating > 0) {
			dom.likeValue.addClass('rating_plus'); 
			rating = '+' + rating;
		} else if (rating < 0) {
			dom.likeValue.addClass('rating_minus');
		}
		dom.likeValue.text(rating);
		$.post("/api/comment_like", {id: dom.id, state: state}, function(data){
			if (data.response != state) {
				dom.like.html(bakup);
				snp_message_danger(data.error);
			}
		}, "json");
		return false;
	});

	$('body').on('click', '.snp-comment-minus',function(){
		var dom = snp_comment_dom(this);
		var bakup = dom.like.html();
		var rating = Number(dom.likeValue.text());
		if ($(this).hasClass('active')) {
			state = 4;
			rating += 1;
		} else {
			state = 3;
			rating -= 1;
			if (dom.likePlus.hasClass('active')) {
				dom.likePlus.removeClass('active');
				rating -= 1;
			}
		}
		$(this).toggleClass('active');
		dom.likeValue.attr('class', '');
		if (rating > 0) {
			dom.likeValue.addClass('rating_plus'); 
			rating = '+' + rating;
		} else if (rating < 0) {
			dom.likeValue.addClass('rating_minus');
		}
		dom.likeValue.text(rating);
		$.post("/api/comment_like", {id: dom.id, state: state}, function(data){
			if (data.response != state) {
				dom.like.html(bakup);
				snp_message_danger(data.error);
			}
		}, "json");
		return false;
	});

	$('body').on('click', '.snp-comment-claim',function() {
		var dom = snp_comment_dom(this);
		snp_comment_ajax({
			url: '/api/comment_claim',
			data: {id: dom.id},
			success: function(data){ 
				snp_message_success(data.text);
			}
		});
		return false;
	});

	$('body').on('click', '.snp-comment-bookmarks',function() {
		var dom = snp_comment_dom(this);
		snp_comment_ajax({
			url: '/api/comment_bookmarks',
			data: {id: dom.id},
			success: function(data){ 
				dom.bookmark.toggleClass('active');
				dom.bookmark.attr('title', data.title);
			}
		});
		return false;
	});

	/* submenu */
	$('body').on('click', '.snp-comment-more-triger',function() {var el = $(this).next('.snp-comment-more-popup');$('.snp-comment-more-popup').not(el).hide();$(el).toggle();return false;});
	$('body').on('click', '.snp-comment-more-popup a',function() {$(this).closest(".snp-comment-more-popup").hide();});
	$("body").click(function(e) {if ($(e.target).closest(".snp-comment-more-popup").length == 0) {$(".snp-comment-more-popup").hide();}});

	/* copy link */
	$(document).ready(function(){
		var comment_clipboard = new ClipboardJS('.snp-comment-url', {text: function(trigger){return $(trigger).attr('href');}});
		comment_clipboard.on('success', function(e){snp_message_success('<p> </p>');});	
		comment_clipboard.on('error', function(e){snp_message_danger('<p>  </p>');});
	});

	$('body').on('click', '.snp-comment-url', function(){return false;});
});

$(function(){	
	var $els = $('.js-load-tpl');
	if ($els.length > 0) {
		$els.each(function(){ 
			var tr = this;
			$.ajax({
				type: "POST",
				url: "/api/" + $(tr).data('name'),
				success: function(data){ 
					$(tr).html(data);	
				}
			});
		});
	}

	$("a.fancybox").fancybox({
		helpers: {
			overlay: {locked: false
			},
			title: {
				type: 'inside',
				position: 'bottom'
			}
		}
	});

	var i,
		imgs=$(".shadow, .image"),
		imgs_count=imgs.length;

	if (imgs_count>0) {
		imgs.each(function(){
			$(this).wrap('<a class="lightBox" rel="gallery" href="'+this.src+'"></a>');
		});
		$('a.lightBox').fancybox({
			helpers: {
				overlay: {locked: false
				},
				title: {
					type: 'inside',
					position: 'bottom'
				}
			}
		});
	}	
});