function snp_message_danger(massege) {
	var item = $('<div class="snp_alert-danger">' + massege + '</div>');
	item.appendTo($('.snp_alert-popup')).delay(5000).slideUp(200, function(){item.remove()});
}
function snp_message_success(massege) {
	var item = $('<div class="snp_alert-success">' + massege + '</div>');
	item.appendTo($('.snp_alert-popup')).delay(5000).slideUp(200, function(){item.remove()});
}

$.ajaxSetup({
	error: function (jqXHR, exception) {
        var msg = '';
        if (jqXHR.status === 0) {
            msg = '<p>Not connect. Verify Network.</p>';
        } else if (jqXHR.status == 404) {
            msg = '<p>Requested page not found (404).</p>';
        } else if (jqXHR.status == 500) {
            msg = '<p>Internal Server Error (500).</p>';
        } else if (exception === 'parsererror') {
            msg = '<p>Requested JSON parse failed.</p>';
        } else if (exception === 'timeout') {
            msg = '<p>Time out error.</p>';
        } else if (exception === 'abort') {
            msg = '<p>Ajax request aborted.</p>';
        } else {
            msg = '<p>Uncaught Error. ' + jqXHR.responseText + '</p>';
        }
		snp_message_danger(msg);
    }
});

$(function() {
   $('.sample-auto').load(function(){
		var target = this;
		var content = $(target).contents().find('body');
		$(target).height($(content).outerHeight(true));
		$(content).on("DOMSubtreeModified click", function (event){
			setTimeout(function(){$(target).stop().animate({height: $(content).outerHeight(true)}, 200);}, 500);
		});
	});

	$.ajax({
		type: "POST",
		url: "/api/getmenu",
		success: function(data){ 
			// topmenu
			$('#load-menu-top').replaceWith(data.top);
			var top_menu_timer;
			$('.top_menu').mouseenter(function(){
				top_menu_timer = setTimeout(function(){$('.top_menu').addClass('js-hover')}, 500);
			});
			$('.top_menu').mouseleave(function(){
				clearTimeout(top_menu_timer);
				$('.top_menu').removeClass('js-hover')
			});					
		}
	});	
	

	$('.content button, form .link-send').click(function(){
		$("#append-input").remove();
		if ($(this).attr('name')) {
			var form = $(this).parents('form').append(
				$("<input id='append-input' type='hidden'>").attr({
					name: $(this).attr('name'), 
					value: $(this).attr('value')
				})
    	    );
    	 }       
	});
	
	$('form .link-send').click(function(){
		$(this).parents('form').submit();
	});
});

function showSample(id) {
	var div = '#sample-code-' + id;
	if (document.getElementById('sample-textarea-' + id)) {
		$(div).slideToggle(100);
	} else {
		$.ajax({
			dataType : "html",
			url: "/sample/code/" + id,
			data: {id: id},
			method: "post",
			success: function (data) { 
				$(div).html(data);
				var mixedMode = {
					name: "htmlmixed",
					scriptTypes: [{matches: /\/x-handlebars-template|\/x-mustache/i,
					mode: null},
					{matches: /(text|application)\/(x-)?vb(a|script)/i,
					mode: "vbscript"}]
				};
				CodeMirror.fromTextArea(document.getElementById('sample-textarea-' + id), {
					lineNumbers: true,
					mode: mixedMode,
					selectionPointer: true
				});	
			}             
		});
	}
}

function refreshSample(id) {
	$('#sample-' + id)[0].contentWindow.location.reload(true);
}


$(document).ready(function() {
    $('body').on('click', '.form-number-minus', function(){
        var $input = $(this).next();
        var count = parseInt($input.val()) - 1;
        count = count < 1 ? 1 : count;
        $input.val(count).change();
        return false;
    });
   
    $('body').on('click', '.form-number-plus', function(){
        var $input = $(this).prev();
        $input.val(parseInt($input.val()) + 1).change();
        return false;
    });
    
    $('body').on('dragenter dragover', '.form-drop', function(){ 
    	$(this).addClass('drag');
    });

    $('body').on('dragleave', '.form-drop', function(){ 	 
		$(this).removeClass('drag');
	});

    $('body').on('change', '.form-drop-control', function(){ 	 
		$(this).parents('.form-drop').addClass('drag');
		$(this).parents('.form-drop').find('.form-drop-text').html('<span class="form-drop-filename"> :</span><br>' + this.files[0].name);
	});
	
    $('body').on('dragenter dragover', '.form-drop-noselected', function(){ 
    	$(this).addClass('drag');
    });

    $('body').on('dragleave', '.form-drop-noselected', function(){ 	 
		$(this).removeClass('drag');
	});
	
	$('.snp_tabs .snp_tabs-nav li:first-child a').addClass('active');
	var tab = $('.snp_tabs .snp_tabs-items > div');  
	$('.snp_tabs .snp_tabs-nav a').click(function(){
		$(this).parents('.snp_tabs').find('.snp_tabs-item').hide(); 
		tab.filter(this.hash).show(); 
		$(this).parents('.snp_tabs').find('.snp_tabs-nav a').removeClass('active');
		$(this).addClass('active');
		return false;
	});
	
	$('.snp-dropdown > li > a').click(function(){
		$(this).parent().toggleClass('active');
		$('.snp-dropdown > li').not($(this).parent()).removeClass('active');
		return false;
	});
	$('.snp-dropdown-menu a').click(function(){
		$(this).parent().toggleClass('active');
		$('.snp-dropdown > li').removeClass('active');
		return false;
	});
	$('body').on('click', function(){
		$('.snp-dropdown > li').removeClass('active');
	});
	$(document).keyup(function(e){
		if (e.key === "Escape"){
			$('.snp-dropdown > li').removeClass('active');
		}
	});

	// search
	$('body').on('click', '.snp-search-icon', function(){var $target = $('.snp-search-overlay');if ($target.is(':hidden')){$('body').addClass('snp-search-body');$target.fadeIn(200, function(){$target.find('input').focus();});} else {$('body').removeClass('snp-search-body');$target.fadeOut(200);}return false;});
	$('body').on('click', '.snp-search-close', function(){$('body').removeClass('snp-search-body');$('.snp-search-overlay').fadeOut(200);return false;});
	$('.snp-search-input').on('keyup', function(){var search = $(this).val();setTimeout(function(){if ((search != '') && (search.length > 1)){snp_send_search();}}, 500);});
	function snp_send_search(){$.ajax({url: 'https://snipp.ru/api/search',method: 'post',dataType: 'html',data: {q: $('.snp-search-input').val()},success: function(data){ $('.snp-search-result').html(data);}});}
	$('body').on('click', '.snp-search-btn', function(){snp_send_search();return false;});
	$('body').on('keyup', '.snp-search-input', function(e){if (e.keyCode === 13) {snp_send_search();}});
	
});

function open_codepen(id) {
	$.ajax({
		dataType : "html",
		url: "/sample/codepen/" + id,
		data: {id: id},
		method: "post",
		success: function (data) { 
			$('#codepen-' + id).html(data);
			$('#codepen-' + id + ' form').submit();           
		}
	});
	return false;
}

function open_jsfiddle(id) {
	$.ajax({
		dataType : "html",
		url: "/sample/jsfiddle/" + id,
		data: {id: id},
		method: "post",
		success: function (data) { 
			$('#codepen-' + id).html(data);
			$('#codepen-' + id + ' form').submit();           
		}
	});
	return false;
}

/* login */
function snp_form_login(hash) { 
	var state = $('link[rel="canonical"]').attr('href');
	if (state === undefined || state == ''){
		state = 'https://snipp.ru';
	}
	if (hash != ''){
		state = state + '#' + hash;
	}
	$.fancybox({
		href: '/login/form?state=' + window.btoa(state),
		closeBtn: false,
		margin: 0,
		padding: 0,
		maxWidth: 400,
		autoScale: true,
		transitionIn: 'none',
		transitionOut: 'none',
		type: 'ajax',
		helpers: {
			overlay: {
				locked: false
			}
		}
	});
	return false;
}

